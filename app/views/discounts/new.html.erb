<h3>Create a New Discount</h3>
</br>
<%= form_with(
  model: @bulk_discounts,
  local: true,
  data: { turbo: false },
  url: merchant_discounts_path(@merchant)
  )do |f| %>
  <%= f.label :percentage_discount %>
  <%= f.text_field :percentage_discount %></br>
  <%= f.label :quantity_threshold %>
  <%= f.text_field :quantity_threshold %></br>
  <%= f.submit "Submit" %>
<% end %>